<form method="post" action="{{ path('chambre_res_delete', {'id': chambre_re.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ chambre_re.id) }}">
    <button class="btn">Delete</button>
</form>
